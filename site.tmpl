<html style="flex: 1 0 auto;">
    <head>
        <link rel="stylesheet" href="/js/materialize.min.css">
        <link rel="icon" href="/js/Tux.png">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <script src="/js/jquery.min.js"></script>
        <script src="/js/materialize.min.js" async></script>
        <style>
            body {
                margin: 0;
                min-height: 100vh;
            }
            #bleh {
                display: flex;
                flex-direction: column;
            }
            .pgr {
                background: #2e0040;
            }
            .ps {
                background: #50245d;
            }
            #title {
                color: white;
            }
            h1, h2, h3, h4, h5, h6, p {
                color: white;
            }
            html, body, h1, h2, h3, h4, h5, h6, div {
                padding: 0;
                border: 0;
                margin: 0;
            }
            div.collapsible-header {
                background-color: black;
                color: white;
            }
            li.collection-item {
                background-color: black !important;
                color: white !important;
            }
            code {
                background-color: grey;
            }
            .purp {
                background-color: rgb(46, 0, 64) !important;
            }
            .dropdown-content li>a {
                color: #2e0040
            }
            .dropdown-content li>span {
                color: #2e0040;
            }
            .side-nav {
                position: fixed;
                width: 200px;
                left: 0;
                top: 0;
                margin: 0;
                -webkit-transform: translateX(-100%);
                transform: translateX(-100%);
                height: 100%;
                height: calc(100% + 60px);
                height: -moz-calc(100%);
                padding-bottom: 60px;
                background-color: #28122e;
                z-index: 999;
                overflow-y: auto;
                will-change: transform;
                -webkit-backface-visibility: hidden;
                backface-visibility: hidden;
                -webkit-transform: translateX(-105%);
                transform: translateX(-105%);
            }           
            .side-nav li>a {
                color: rgba(255, 255, 255, 0.87);
            }
            .dropdown-content li>a {
                color: #2e0040;
            }
            .side-nav li>a>i.material-icons {
                color: rgba(255, 255, 255, 0.54);
                margin: 0 0px 0 0;
            }
        </style>
        <title>{{.Title}}</title>
    </head>
    <body>
        <div id="bleh" style="padding: 0;border: 0;margin: 0;">
            <header>
            <ul id="dropdown1" class="dropdown-content">
                <li><a href="/Developing.html">Developing</a></li>
                <li><a href="/Package%20Generator.html">Package Generator</a></li>
            </ul>
                <nav class="purp" id="nav">
                    <div class="nav-wrapper container">
                        <a href="/" class="brand-logo center hide-on-med-and-down"><img src="/js/Panux Logo.png" height=100%></img></a>
                        <a href="/" class="right hide-on-med-and-up show-on-medium-and-down"><img src="/js/Panux Logo.png" height=100%></img></a>
                        <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
                        <ul id="nav-mobile" class="right hide-on-med-and-down">
                            <li><a href="/Home.html">Home</a></li>
                            <li><a class="dropdown-button" href="#!" data-activates="dropdown1">Development<i class="material-icons right">arrow_drop_down</i></a></li>
                            <li><a href="/Community.html">Community</a></li>
                        </ul>
                        <ul class="side-nav" id="mobile-demo">
                            <li><a href="/Home.html">Home</a></li>
                            <li><a class="dropdown-button" href="#!" data-activates="dropdown1">Development<i class="material-icons right">arrow_drop_down</i></a></li>
                            <li><a href="/Community.html">Community</a></li>
                        </ul>
                        <h2 font-family:"Verdana", class="header center-on-small-only", id="Title">{{.Title}}</h2>
                    </div>
                </nav>
            </header>
            <div class="ps" id="pg">
                <div class="container col s12">
                            {{.Page}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script>
            (window.onresize = function() {
                document.getElementById('pg').style.minHeight = window.innerHeight - document.getElementById('pg').offsetTop;
            })();
        </script>
    </body>
</html>
